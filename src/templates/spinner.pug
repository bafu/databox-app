div(style="padding: 16px; display: flex; justify-content: center; margin-top: 200px")
	style.
		#spinner {
			align-self: center;
			animation: rotator 1.4s linear infinite;
		}

		@keyframes rotator {
			0% {
				transform: rotate(0deg);
			}
			100% {
				transform: rotate(270deg);
			}
		}

		.path {
			stroke-dasharray: 187;
			stroke-dashoffset: 0;
			stroke: #ff8f00;
			transform-origin: center;
			animation: dash 1.4s ease-in-out infinite
		}

		@keyframes dash {
			0% {
				stroke-dashoffset: 187;
			}
			50% {
				stroke-dashoffset: 46.75;
				transform: rotate(135deg);
			}
			100% {
				stroke-dashoffset: 187;
				transform: rotate(450deg);
			}
		}
	svg#spinner(width="35px", height="35px", viewBox="0 0 66 66", xmlns="http://www.w3.org/2000/svg")
		circle.path(fill="none", stroke-width="6", stroke-linecap="square", cx="33", cy="33", r="30")

