{
  "name": "databoxApp",
  "displayName": "Databox",
  "version": "0.1.0",
  "description": "Databox App",
  "main": "index.js",
  "scripts": {
    "watch": "pug -cw --name-after-file src/templates/ -o www/js/templates/ & pug -w src/*.pug -o www/",
    "build-cm": "pug -c --name-after-file src/templates/ -o ../databox/core-container-manager/src/www/https/js/templates/ && pug src/*.pug -o ../databox/core-container-manager/src/www/https/ && pug src/http/*.pug -o ../databox/core-container-manager/src/www/http/"
  },
  "author": "Apache Cordova Team",
  "license": "Apache-2.0",
  "dependencies": {
    "cordova-android": "^6.4.0",
    "cordova-browser": "^5.0.1",
    "cordova-ios": "^4.5.4",
    "cordova-plugin-compat": "^1.2.0",
    "cordova-plugin-qrscanner": "^2.5.0",
    "cordova-plugin-sensingKit": "git+https://github.com/ktg/cordova-plugin-sensingkit.git",
    "cordova-plugin-statusbar": "^2.3.0",
    "cordova-plugin-swift-support": "^3.1.1",
    "cordova-plugin-whitelist": "^1.3.3",
    "material-components-web": "^0.26.0",
    "navigo": "^6.0.0",
    "pug": "^2.0.0-rc.4",
    "pug-cli": "^1.0.0-alpha6"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-qrscanner": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-compat": {},
      "cordova-plugin-sensingKit": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  },
  "false": {}
}