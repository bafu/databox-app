        "cordova-plugin-statusbar": "^2.2.3",
        "cordova-plugin-whitelist": "^1.3.2",
        "material-components-web": "^0.20.0",
        "navigo": "^5.3.1",
        "pug": "^2.0.0-rc.4",
        "pug-cli": "^1.0.0-alpha6",
        "cordova-plugin-sensingKit": "http://github.com/ktg/cordova-plugin-sensingKit.git"
